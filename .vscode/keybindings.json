[
  {
    "key": "ctrl+shift+v",
    "command": "markdown.showPreview",
    "when": "editorLangId == markdown"
  },
  {
    "key": "ctrl+k v",
    "command": "markdown.showPreviewToSide",
    "when": "editorLangId == markdown"
  },
  {
    "key": "alt+m",
    "command": "markdown.showPreview",
    "when": "editorLangId == markdown"
  },
  {
    "key": "ctrl+shift+c ctrl+shift+o",
    "command": "vscode-ext-commit.showOutput",
    "when": "!terminalFocus"
  }
]
